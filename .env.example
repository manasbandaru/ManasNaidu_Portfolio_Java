# Environment variables for Java Developer Portfolio
# Copy this file to .env and update with your values

# Site Configuration
VITE_SITE_URL=https://java-developer-portfolio.netlify.app
VITE_SITE_NAME=Java Developer Portfolio
VITE_SITE_DESCRIPTION=Manas Naidu - Java Full Stack Developer Portfolio - 5+ years experience in Spring Boot, Hibernate, REST APIs, microservices, and cloud platforms
VITE_SITE_KEYWORDS=Java Developer, Spring Boot, Hibernate, REST API, Microservices, Full Stack Developer

# Contact form configuration
VITE_CONTACT_API_ENDPOINT=/api/contact
VITE_CONTACT_RECIPIENT_EMAIL=manasnaidu.jsd@gmail.com
VITE_ENABLE_REAL_CONTACT_SUBMISSION=false
VITE_MAX_MESSAGE_LENGTH=2000
VITE_CONTACT_RATE_LIMIT_DELAY=5000

# Social Media Links
VITE_GITHUB_URL=https://github.com/Manasbandaru
VITE_LINKEDIN_URL=https://www.linkedin.com/in/manasnaidujsd/

# Portfolio Configuration
VITE_PORTFOLIO_NAME=Manas Naidu
VITE_PORTFOLIO_TITLE=Senior Java Developer
VITE_YEARS_EXPERIENCE=3

# Analytics (optional)
VITE_GA_TRACKING_ID=your-google-analytics-id
VITE_HOTJAR_ID=your-hotjar-id

# API endpoints (if needed)
VITE_API_BASE_URL=https://your-api.com/api

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CONTACT_FORM=true
VITE_ENABLE_3D_BACKGROUND=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_BUNDLE_ANALYZER=false

# Performance Configuration
VITE_WEBGL_PARTICLE_COUNT=1000
VITE_MOBILE_PARTICLE_COUNT=300
VITE_ENABLE_REDUCED_MOTION=true